X = matrix(seq(1,6000), 600, 10)/6000;
ssX_V = matrix(seq(1,40), 10, 4)/40;
P = matrix(seq(1,3000), 600, 5)/3000;
K = 4;

while(FALSE){}
Q = P[,1:K] * (X %*% ssX_V);
R = t(X) %*% (Q - P[,1:K] * rowSums(Q));
if( nrow(R)!=10 | ncol(R)!=4 )
  stop("Wrong output dimensions.");

write(R, $1)
