printAndAssign = function(Matrix[Double] X) return (Matrix[Double] Y) {
	if( sum(X) > 0 )
     print("sum(X) = " + sum(X));
  Y = X;
}


X = matrix(0.5, 600, 10);
ssX_V = matrix(0.9, 10, 1);
P = matrix(0.7, 600, 2);

Q = P[,1:1] * (X %*% ssX_V);
Y = X + 2;
Y = printAndAssign(X);
Y = printAndAssign(X);
R = t(X) %*% (Q - P[,1:1] * rowSums(Q));

write(R, $1);
