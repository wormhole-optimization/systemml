S = matrix(1,rows=100,cols=1);
while(FALSE){}

A = diag(S);
A = removeEmpty (target = A, margin = "rows");
B = A %*% S;
C = table (B, seq (1, nrow(A), 1));
R = removeEmpty (target = C, margin = "rows");
R = R %*% A;

while(FALSE){} 

print(sum(R));
write(R, $1);       