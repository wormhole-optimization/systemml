dim0 = $1
dim1 = $2

A = Rand(rows=dim0, cols=dim1, min=-1, max=1, pdf="uniform", seed=0)
A_sparse = Rand(rows=dim0, cols=dim1, min=-1, max=1, pdf="uniform", seed=0, sparsity=0.01)
B = Rand(rows=dim0, cols=dim1, min=-1, max=1, pdf="uniform", seed=1)
B_sparse = Rand(rows=dim0, cols=dim1, min=-1, max=1, pdf="uniform", seed=1, sparsity=0.01)

write(A, $3, format=$7)
write(A_sparse, $4, format=$7)
write(B, $5, format=$7)
write(B_sparse, $6, format=$7)

