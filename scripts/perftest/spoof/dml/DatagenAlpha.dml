dim0 = $1
dim1 = $2
dim2 = $3

A = Rand(rows=dim0, cols=dim2, min=-1, max=1, pdf="uniform", seed=0, sparsity=1)
B = Rand(rows=dim1, cols=dim2, min=-1, max=1, pdf="uniform", seed=1, sparsity=1)
C = Rand(rows=dim1, cols=1,    min=-1, max=1, pdf="uniform", seed=2, sparsity=1)
D = Rand(rows=dim1, cols=dim0, min=-1, max=1, pdf="uniform", seed=3, sparsity=0.001)

write(A, $4, format=$8)
write(B, $5, format=$8)
write(C, $6, format=$8)
write(D, $7, format=$8)

